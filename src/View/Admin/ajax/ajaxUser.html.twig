{% for user in users %}

    <tr>
        <th scope="row">{{ loop.index }}</th>
        <td>{{ user.pseudo }}</td>
        <td>{{ user.email }}</td>
        <td>{{ user.create_at }}</td>
        <td>{{ user.update_at }}</td>
        <td>{{ user.role_user == 'ROLE_USER' ? 'Membre' : 'Administrateur' }}</td>
        <td>
            <a href="/admin/edituser/{{ user.id }}">
                <i class="far fa-edit"></i>
            </a>
            /
            <i class="fas fa-trash-alt trash-user" data-id="{{ user.id }}"></i>
        </td>
    </tr>
{% endfor %}
<input id="lengthTable" type="hidden" value="{{ lengthTable }}">